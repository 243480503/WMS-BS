@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");
    $(function () {
        $.ajax({
            url: "/PC_QAManage/QAGet/GetFormJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                $("#form1").formSerialize(data);
                $("#form1").find('.form-control,select,input').attr('readonly', 'readonly');
                $("#form1").find('div.ckbox label').attr('for', '');
            }
        });
    });

</script>

<form id="form1">
    <div style="padding-top: 20px; margin-right: 20px;">
        <table class="form">
            <tr>
                <th class="formTitle">质检单编码</th>
                <td class="formValue">
                    <input id="QACode" name="QACode" type="text" class="form-control required" placeholder="请输入质检单编码" />
                </td>
                <th class="formTitle">质检单状态</th>
                <td class="formValue">
                    <input id="StateName" name="StateName" type="text" class="form-control required" placeholder="请输入质检单编码" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">来源单号</th>
                <td class="formValue">
                    <input id="RefOrderCode" name="RefOrderCode" type="text" class="form-control" placeholder="请输入来源单号" />
                </td>
                <th class="formTitle">入库来源单号</th>
                <td class="formValue">
                    <input id="RefInBoundCode" name="RefInBoundCode" type="text" class="form-control"/>
                </td>
            </tr>
            <tr>
                <th class="formTitle" valign="top" style="padding-top: 5px;">备注</th>
                <td class="formValue" colspan="3">
                    <textarea id="Remark" name="Remark" class="form-control" style="height: 60px;"></textarea>
                </td>
            </tr>
            <tr>
                <th class="formTitle">创建人员</th>
                <td class="formValue">
                    <input id="CreatorUserName" name="CreatorUserName" type="text" class="form-control" />
                </td>
                <th class="formTitle">创建时间</th>
                <td class="formValue">
                    <input id="F_CreatorTime" name="F_CreatorTime" type="text" class="form-control" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">修改人员</th>
                <td class="formValue">
                    <input id="ModifyUserName" name="ModifyUserName" type="text" class="form-control" />
                </td>
                <th class="formTitle">修改时间</th>
                <td class="formValue">
                    <input id="F_LastModifyTime" name="F_LastModifyTime" type="text" class="form-control" />
                </td>
            </tr>
        </table>
    </div>
</form>
