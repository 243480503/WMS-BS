@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");
    $(function () {
        $.ajax({
            url: "/PC_InBoundManage/InBound/GetFormJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                $.checkLogin(data);
                $("#form1").formSerialize(data);
                $("#form1").find('.form-control,select,input').attr('disabled', 'readonly');
            }
        });
    });

</script>

<form id="form1">
    <div style="padding-top: 20px; margin-right: 20px;">
        <table class="form">
            <tr>
                <th class="formTitle">入库单编码</th>
                <td class="formValue">
                    <input id="InBoundCode" name="InBoundCode" type="text" class="form-control required" />
                </td>
                <th class="formTitle">来源单号</th>
                <td class="formValue">
                    <input id="RefOrderCode" name="RefOrderCode" type="text" class="form-control" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">供应商</th>
                <td class="formValue">
                    <input id="SupplierUserName" name="SupplierUserName" type="text" class="form-control" />
                </td>
                <th class="formTitle">单据类型</th>
                <td class="formValue">
                    <input id="InBoundTypeName" name="InBoundTypeName" type="text" class="form-control" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">订单状态</th>
                <td class="formValue">
                    <input id="StateName" name="StateName" type="text" class="form-control" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">创建人员</th>
                <td class="formValue">
                    <input id="CreatorUserName" name="CreatorUserName" type="text" class="form-control" />
                </td>
                <th class="formTitle">创建时间</th>
                <td class="formValue">
                    <input id="F_CreatorTime" name="F_CreatorTime" type="text" class="form-control" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">修改人员</th>
                <td class="formValue">
                    <input id="ModifyUserName" name="ModifyUserName" type="text" class="form-control" />
                </td>

                <th class="formTitle">修改时间</th>
                <td class="formValue">
                    <input id="F_LastModifyTime" name="F_LastModifyTime" type="text" class="form-control" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">备注</th>
                <td class="formValue" colspan="3">
                    <textarea id="Remark" name="Remark" class="form-control" style="height: 60px;"></textarea>
                </td>
            </tr>
        </table>
    </div>
</form>
