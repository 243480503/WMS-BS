<?xml version="1.0" encoding="utf-8"?>
<appSettings>
	<!-- ================== 1：开发系统相关配置 ================== -->
	<!-- 登陆提供者模式：Session、Cookie-->
	<add key="LoginProvider" value="Cookie" />
	<!-- 数据库超时间（秒）-->
	<add key="CommandTimeout" value="180" />
	<!-- ================== 2：系统软件参数配置 ================== -->
	<!-- 联系我们 -->
	<add key="WebSite" value="http://www.wmswcsdev.com" />
	<!-- 源码更新 -->
	<add key="GithubUrl" value="https://github.com/243480503/WMS-BS" />
	<!-- 软件名称 -->
	<add key="SoftName" value="崇令" />
	<!-- 软件通用名称 -->
	<add key="SoftPubName" value="WMS系统" />
	<!-- 公司名称 -->
	<add key="CompanyName" value="崇令智能仓储管理系统" />
	<!-- 软件版本 -->
	<add key="Version" value="V2.0.1" />
	<!-- 图标位置 -->
	<add key="LogoPath" value="../Content/img/LogoSquareWhile.png" />
	<!-- 登录图标与主页面图标缩放大小 -->
	<add key="LogoScale" value="70% 60%" />
	<!-- 欢迎页面,推荐:/Home/Default -->
	<add key="WelcomePage" value="/Home/Default" />
	<!-- 软件授权码-->
	<add key="LicenceKey" value="83eda07f-ead3-4050-bcee-4f41e57a7e16" />
	<!-- ================== 3：外部邮件参数 ================== -->
	<!-- 设置邮箱名称 -->
	<add key="MailName" value="邮件中心" />
	<!-- 设置邮箱地址 -->
	<add key="MailUserName" value="sendbug@ChongLing.cn" />
	<!-- 设置邮箱密码 -->
	<add key="MailPassword" value="123456" />
	<!-- 设置邮箱主机 -->
	<add key="MailHost" value="smtp.ym.ChongLing.com" />
	<!-- ================== 4：必填参数 ================== -->
	<!--由于IIS定时器回收，需在IIS设置“固定时间间隔(分钟)=0”，且“闲置超时(分钟)=0”,或设置以下参数-->
	<!--是否允许释放应用程序池（不允许则重新激活应用程序池）-->
	<add key="IsFreeWebPool" value="true" />
	<add key="WebHost" value="http://localhost" />
	<add key="WebPort" value="3000" />

</appSettings>